	</head>
	<body>
	<%- include('head') %>
	<div class="de-top"></div>
	<div class="de-top1"></div>
	<div class="decontent">
		<div class="de-left" style="background: <%= products.url %>; background-size:100% 100% ;"></div>
		<div class="de-right">
			<h1><%= products.head %></h1>
			<p><%= products.data %></p>
			<ul>
				<% for(var j=0;j<products.detail.length;j++){ %> 
				<li><%=  products.detail[j] %></li>
				<% } %>
			</ul>
		</div>
	</div>
	<% if (product.length>1) { %> 
		<div class="de-bottom">
			<ul>
				<% for(var i=0;i<product.length;i++){ %> 
					<a href="<%= i %>detail">
				<li>
					<div class="bacg" style="background:<%= product[i].url %>;background-size:100% 100% ;"></div>
					<p><%= product[i].head %> </p>
				</li>
				</a>
				 <% } %>
			</ul>
		</div>
		<% } %>
	<%- include('form') %>
	<%- include('foot') %>
	</body>
</html>